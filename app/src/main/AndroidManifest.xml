<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mazrepartotienda">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconomaz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconomaz"
        android:supportsRtl="true"
        android:theme="@style/Theme.MazRepartoTienda">
        <activity android:name=".MainActivity_detalle"></activity>
        <activity android:name=".MainActivity_Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity_Login" />
        <activity android:name=".MainActivity_Pedido" />
        <activity android:name=".MainActivity_list" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>